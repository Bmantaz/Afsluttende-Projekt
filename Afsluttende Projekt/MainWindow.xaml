<Window x:Class="Afsluttende_Projekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Afsluttende_Projekt"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Background>
        <!-- PLACE HOLDER SKIFT DENNE -->
        <ImageBrush ImageSource="Assets/Baggrund.jpg"/>
    </Window.Background>
    <Grid>
        <!-- "Medarbejder Login" knap -->
        <Button x:Name="btnMedarbejderLogin"
                Content="Medarbejder Login"
                Width="150" Height="30"
                HorizontalAlignment="Right" VerticalAlignment="Top"
                Margin="0,10,10,0"
                Click="btnMedarbejderLogin_Click"/>
    </Grid>
</Window>
